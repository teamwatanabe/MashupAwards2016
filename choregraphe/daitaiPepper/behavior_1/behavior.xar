    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box


    def get_random_str(self, length):
        import random, string
        return ''.join([random.choice(string.ascii_letters + string.digits) for i in range(length)])

    def set_uploaddata(self):
        import json
        # Pepper ID
        Camera_ID = self.memory.getData("RobotConfig/Head/FullHeadId")
        #encode multipart format
        BOUNDARY_STR = self.get_random_str(42)
        content_type = 'multipart/form-data; boundary=' + BOUNDARY_STR
        lines = []
        # multipart - parameters
        params = {
            'Camera_ID' : Camera_ID,
            'request':self.getParameter("Type"),
            'Group':self.memory.getData("GLData/Group"),
            'username':self.memory.getData("GLData/User"),
            'password':self.memory.getData("GLData/Password")
        }
        for (key, value) in params.items():
            lines.append(bytes('--' + BOUNDARY_STR))
            lines.append(bytes('Content-Disposition: form-data; name="%s"' % key))
            lines.append(b'')
            lines.append(value)
        # multipart - endline
        lines.append(bytes('--' + BOUNDARY_STR + '--'))
        lines.append(b'')
        try:
            body = bytes("\r\n").join(lines)
        except:
            return None, None
        header = {"Content-Type" : content_type}
        return body, header]]>
