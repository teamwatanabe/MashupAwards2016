                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        self.memory = None

    def onInput_onStart(self):
        self.memory.insertData("GLData/ServerIP", self.getParameter("ServerIP"))
        self.memory.insertData("GLData/User", self.getParameter("username"))
        self.memory.insertData("GLData/Password", self.getParameter("password"))
        self.memory.insertData("GLData/Group", self.getParameter("Group"))
        self.onStopped()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
