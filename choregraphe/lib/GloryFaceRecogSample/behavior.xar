                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self, p):
        #import xml.etree.ElementTree as ET
        import requests

        #レスポンスの取得
        data = p

        if not data:
            self.TypeNone(p)

        else:
            #Typeタグの取得
            type = self.xmlRead(data,'Type')

            #Typeタグの内容が"Accepted"もしくは"Success"の場合
            if  type == "Accepted" or type == "Success":
                #IDの取得
                idValue = unicode(self.xmlRead(data,'Id')).encode('utf-8')
                #名前の取得
                name = self.xmlRead(data,'Name')
                #性別の取得
                gender = str(self.xmlRead(data,'Gender'))
                #年齢の取得
                age = str(self.xmlRead(data,'Age'))
                #年齢範囲の取得
                ageRange = str(self.xmlRead(data,'AgeRange'))


                outValue = name.encode('utf-8') +","+ gender +","+ age +","+ ageRange

                self.TypeSuccess(outValue)

            #Typeタグの内容が"Failed"もしくは"Rejected"もしくは"NoResult"の場合
            elif type == "Failed" or type == "Rejected" or type == "NoResult":
                self.TypeReject()
            #Typeタグの内容が"NoFace"の場合
            elif type == "NoFace":
                self.TypeNoFace()
            #Typeタグの内容が上記以外の場合
            else:
                self.TypeNone(p)

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def xmlRead(self, xmlTree, tag):
        import xml.etree.ElementTree as ET

        # '&'が実体参照していないとエラーが出るため正規表現に置換
        # ついでに改行も置換
        #self.logger.info(xmlTree)
        re_data = (str(xmlTree).replace('&','&amp;')).replace('\r\n','').replace('\t','').replace('\n','').replace('ï»¿','')

        # XMLツリーのルートを取得
        parser = ET.XMLParser(encoding="utf-8")
        root = ET.fromstring(re_data, parser=parser)

        return root.findtext(".//"+str(tag))]]>
